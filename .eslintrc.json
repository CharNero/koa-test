{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "standard",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": 1,
    "no-useless-escape": 0,
    "no-prototype-builtins": 0
  }
}
